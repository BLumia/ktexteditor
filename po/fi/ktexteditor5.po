msgstr "Vieritä rivi ylös"

#: view/kateview.cpp:986
#, kde-format
msgid "Move to Next Line"
msgstr "Siirry seuraavalle riville"

#: view/kateview.cpp:992
#, kde-format
msgid "Move to Previous Line"
msgstr "Siirry edelliselle riville"

#: view/kateview.cpp:998
#, kde-format
msgid "Move Cursor Right"
msgstr "Siirrä kohdistin oikealle"

#: view/kateview.cpp:1004
#, kde-format
msgid "Move Cursor Left"
msgstr "Siirry kohdistin vasemmalle"

#: view/kateview.cpp:1010
#, kde-format
msgid "Select to Next Line"
msgstr "Valitse seuraavalle riville"

#: view/kateview.cpp:1016
#, kde-format
msgid "Scroll Line Down"
msgstr "Vieritä rivi alas"

#: view/kateview.cpp:1022
#, kde-format
msgid "Scroll Page Up"
msgstr "Vieritä sivu ylös"

#: view/kateview.cpp:1028
#, kde-format
msgid "Select Page Up"
msgstr "Valitse sivu ylös"

#: view/kateview.cpp:1034
#, kde-format
msgid "Move to Top of View"
msgstr "Siirry näkymän alkuun"

#: view/kateview.cpp:1040
#, kde-format
msgid "Select to Top of View"
msgstr "Valitse näkymän alkuun"

#: view/kateview.cpp:1046
#, kde-format
msgid "Scroll Page Down"
msgstr "Vieritä sivu alas"

#: view/kateview.cpp:1052
#, kde-format
msgid "Select Page Down"
msgstr "Valitse sivu alas"

#: view/kateview.cpp:1058
#, kde-format
msgid "Move to Bottom of View"
msgstr "Siirry näkymän loppuun"

#: view/kateview.cpp:1064
#, kde-format
msgid "Select to Bottom of View"
msgstr "Valitse näkymän loppuun"

#: view/kateview.cpp:1070
#, kde-format
msgid "Move to Matching Bracket"
msgstr "Siirry vastaavan sulkeeseen"

#: view/kateview.cpp:1076
#, kde-format
msgid "Select to Matching Bracket"
